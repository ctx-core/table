{
	"name": "@ctx-core/table",
	"version": "13.0.6",
	"description": "ctx-core table",
	"keywords": [
		"ctx-core",
		"table"
	],
	"homepage": "https://github.com/ctx-core/table#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/table/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/table/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.30",
		"@ctx-core/combinators": "workspace:^8.0.24",
		"@ctx-core/dom": "workspace:^11.0.37",
		"@ctx-core/function": "workspace:^20.2.10",
		"@ctx-core/object": "workspace:^21.0.29",
		"@ctx-core/store": "workspace:^27.0.43",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}