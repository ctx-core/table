{
	"name": "@ctx-core/table",
	"version": "12.0.14",
	"description": "ctx-core table",
	"keywords": [
		"ctx-core",
		"table"
	],
	"homepage": "https://github.com/ctx-core/table#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/table/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/table/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.13",
		"@ctx-core/combinators": "workspace:^8.0.13",
		"@ctx-core/dom": "workspace:^11.0.15",
		"@ctx-core/function": "workspace:^20.0.12",
		"@ctx-core/object": "workspace:^21.0.12",
		"@ctx-core/store": "workspace:^27.0.13",
		"esm": "^3.2.25",
		"svelte": "3.40.1"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}