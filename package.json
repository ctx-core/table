{
	"name": "@ctx-core/table",
	"version": "14.0.7",
	"description": "ctx-core table",
	"keywords": [
		"ctx-core",
		"table"
	],
	"homepage": "https://github.com/ctx-core/table#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/table/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/table.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.39",
		"@ctx-core/combinators": "workspace:^8.0.32",
		"@ctx-core/dom": "workspace:^11.0.46",
		"@ctx-core/function": "workspace:^20.2.18",
		"@ctx-core/object": "workspace:^22.0.6",
		"@ctx-core/store": "workspace:^27.0.53",
		"svelte": "^3.44.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
